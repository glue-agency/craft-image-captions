{
    "name": "glue-agency/craft-image-captions",
    "description": "Automatically generate image captions",
    "type": "craft-plugin",
    "require": {
        "php": ">=8.1",
        "craftcms/cms": "^4.4.0",
        "guzzlehttp/guzzle": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "GlueAgency\\ImageCaption\\": "src/"
        }
    },
    "extra": {
        "handle": "image-captions",
        "name": "Image Captions",
        "developer": "Glue Agency",
        "documentationUrl": "",
        "class": "GlueAgency\\ImageCaption\\ImageCaption"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
